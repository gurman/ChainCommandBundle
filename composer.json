  "type": "symfony-bundle",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "Viktor Holovko",
      "email": "viktor.gurman@gmail.com"
    }
  ],
  "require": {
